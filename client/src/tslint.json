{
    "extends": "../tslint.json",
    "rules": {
      "quotemark": [false, "single", "double"],
      "max-line-length": [true, 160],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
